<Application x:Class="LiteApp.Bizcard.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LiteApp.Bizcard"
             xmlns:resx="clr-namespace:LiteApp.Bizcard.Resources">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:AppBootstrapper x:Key="bootstrapper" />
                    <resx:ApplicationResources x:Key="AppResx"/>
                    
                </ResourceDictionary>
                <ResourceDictionary x:Name="ThemeColor" Source="Views/Themes/BlueThemeStyles.xaml"/>
                <ResourceDictionary Source="Views/ControlStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
